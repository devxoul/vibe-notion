{
  "name": "agent-notion",
  "version": "0.1.0",
  "description": "Notion API CLI for AI agents",
  "type": "module",
  "module": "dist/cli.js",
  "main": "dist/cli.js",
  "bin": {
    "agent-notionbot": "./src/platforms/notionbot/cli.ts",
    "agent-notion": "./src/platforms/notion/cli.ts"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "bun scripts/postbuild.ts",
    "test": "bun test src/",
    "test:e2e": "bun test e2e/",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write --unsafe .",
    "format": "biome format --write .",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@notionhq/client": "latest",
    "better-sqlite3": "latest",
    "commander": "latest"
  },
  "devDependencies": {
    "@biomejs/biome": "latest",
    "@types/better-sqlite3": "latest",
    "@types/bun": "latest",
    "@types/node": "latest",
    "typescript": "latest"
  }
}
