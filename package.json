{
  "name": "vibe-notion",
  "version": "0.1.1",
  "description": "Notion API CLI for AI agents",
  "type": "module",
  "module": "dist/cli.js",
  "main": "dist/cli.js",
  "bin": {
    "vibe-notionbot": "./src/platforms/notionbot/cli.ts",
    "vibe-notion": "./src/platforms/notion/cli.ts"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "bun scripts/postbuild.ts",
    "test": "bun test src/",
    "test:e2e": "bun test --config /dev/null e2e/",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write --unsafe .",
    "format": "biome format --write .",
    "prepublishOnly": "bun run build && bun scripts/prepublish.ts",
    "postpublish": "git checkout package.json"
  },
  "dependencies": {
    "@notionhq/client": "latest",
    "@tryfabric/martian": "^1.2.4",
    "better-sqlite3": "latest",
    "commander": "latest",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devxoul/vibe-notion.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "latest",
    "@types/better-sqlite3": "latest",
    "@types/bun": "latest",
    "@types/node": "latest",
    "typescript": "latest"
  }
}
